{
  "Dhcp4": {
      "loggers": [
        {
          "name": "kea-dhcp4",
          "output_options": [{"output": "/var/log/kea/logs.log"}],
          "severity": "DEBUG"
        }
    ],

    "interfaces-config": {
      "interfaces": [ "eth0" ]
    },

    "lease-database": {
        "type": "memfile",
        "persist": true,
        "name": "/var/lib/kea/lista-concesiones.csv",
        "lfc-interval": 300,
        "max-row-errors": 100
    },

    "valid-lifetime": 600,
    "renew-timer": 300,
    "rebind-timer": 500,
    "authoritative": true,

    "subnet4": [
      {
        "id": 1,
        "interface": "eth0",
        "subnet": "192.168.10.0/24",
        "pools": [{ "pool": "192.168.10.30-192.168.10.32" },
                  { "pool": "192.168.10.101-192.168.10.230" }],
        "option-data":[
          {
            "name": "domain-name-servers",
            "space": "dhcp4",
            "data": "192.168.10.30"
          }
        ],
        "reservations": [
          {
            "hw-address": "02:42:ac:11:00:ff",
            "ip-address": "192.168.10.30"
          },
          
          {
            "hw-address": "02:42:ac:11:00:10",
            "ip-address": "192.168.10.31"
          }
        ]
      },

      {
        "id": 2,
        "interface": "eth0",
        "subnet": "192.168.11.0/24",
        "pools": [{ "pool": "192.168.11.0-192.168.11.254" }],
        "option-data":[
          {
            "name": "domain-name-servers",
            "space": "dhcp4",
            "data": "192.168.10.30"
          }
        ]
      },

      {
        "id": 3,
        "interface": "eth0",
        "subnet": "192.168.57.0/24",
        "pools": [{ "pool": "192.168.57.0-192.168.57.254" }],
        "option-data":[
          {
            "name": "domain-name-servers",
            "space": "dhcp4",
            "data": "192.168.10.30"
          }
        ]
      }
    ]
  }
}
